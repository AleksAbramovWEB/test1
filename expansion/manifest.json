{
  "name": "Расширение google chrome",
  "description": "Написать браузерное расширение для Хрома",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "ПОДСВЕТИТЬ ING"
  }

}